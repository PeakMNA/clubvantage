# ClubVantage API Environment Variables

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/clubvantage
DIRECT_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/clubvantage

# Redis
REDIS_URL=redis://localhost:6379
REDIS_KEY_PREFIX=clubvantage:

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production
JWT_REFRESH_EXPIRES_IN=30d
BCRYPT_ROUNDS=12
LOCKOUT_THRESHOLD=5
LOCKOUT_DURATION=30

# Meilisearch (Search)
MEILISEARCH_HOST=http://localhost:7700
MEILISEARCH_API_KEY=your-meilisearch-api-key

# Supabase (Auth + Storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret
SUPABASE_STORAGE_BUCKET=clubvantage
COOKIE_DOMAIN=localhost

# Resend (Email)
RESEND_API_KEY=re_xxxxxxxxxxxx
EMAIL_FROM=noreply@clubvantage.app
EMAIL_FROM_NAME=ClubVantage

# Stripe (Payments)
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxx

# Sentry (Error Tracking)
SENTRY_DSN=https://xxxx@sentry.io/xxxx
