{
  "name": "@clubvantage/api-client",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./types": "./src/types/index.ts",
    "./provider": "./src/provider/index.ts",
    "./auth": "./src/auth/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts && node -e \"const fs = require('fs'); const f = './src/hooks/generated.ts'; let c = fs.readFileSync(f, 'utf8'); if (!c.startsWith(\\\"'use client'\\\")) { c = \\\"'use client';\\\\n\\\" + c.replace(/'use client';\\\\n?/g, ''); fs.writeFileSync(f, c); }\"",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.60.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-ws": "^5.16.0"
  },
  "devDependencies": {
    "@clubvantage/config": "workspace:*",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@types/react": "^19.0.0",
    "next": "^15.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": "^19.0.0"
  }
}
