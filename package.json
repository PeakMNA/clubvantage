{
  "name": "clubvantage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "kill-ports": "node -e \"const {execSync} = require('child_process'); ['3000','3001','3002','3003','3004','3005'].forEach(p => { try { execSync('lsof -ti:' + p + ' | xargs kill -9', {stdio:'ignore'}); console.log('Killed process on port ' + p); } catch(e) {} })\"",
    "predev": "pnpm kill-ports",
    "dev": "turbo dev",
    "dev:all": "pnpm kill-ports && turbo dev --filter=@clubvantage/application --filter=@clubvantage/platform-manager --filter=@clubvantage/tenant-admin --filter=@clubvantage/marketing --filter=@clubvantage/api",
    "dev:app": "pnpm kill-ports && turbo dev --filter=@clubvantage/application",
    "dev:portal": "pnpm kill-ports && turbo dev --filter=@clubvantage/member-portal",
    "dev:platform": "pnpm kill-ports && turbo dev --filter=@clubvantage/platform-manager",
    "dev:tenant-admin": "pnpm kill-ports && turbo dev --filter=@clubvantage/tenant-admin",
    "dev:marketing": "pnpm kill-ports && turbo dev --filter=@clubvantage/marketing",
    "dev:api": "pnpm kill-ports && turbo dev --filter=@clubvantage/api",
    "dev:services": "pnpm kill-ports && turbo dev --filter='./services/*'",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "pretest:e2e": "node -e \"const {execSync} = require('child_process'); ['3000','3001'].forEach(p => { try { execSync('lsof -ti:' + p + ' | xargs kill -9', {stdio:'ignore'}); console.log('Killed process on port ' + p); } catch(e) {} })\"",
    "db:migrate": "turbo db:migrate",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:seed": "turbo db:seed",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@clubvantage/config": "workspace:*",
    "@playwright/test": "^1.58.0",
    "prettier": "^3.2.0",
    "turbo": "^2.3.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "next-themes": "^0.4.6",
    "recharts": "^3.6.0"
  }
}
