{
  "name": "clubvantage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:all": "turbo dev --filter=@clubvantage/application --filter=@clubvantage/platform-manager --filter=@clubvantage/tenant-admin --filter=@clubvantage/marketing --filter=@clubvantage/api",
    "dev:app": "turbo dev --filter=@clubvantage/application",
    "dev:portal": "turbo dev --filter=@clubvantage/member-portal",
    "dev:platform": "turbo dev --filter=@clubvantage/platform-manager",
    "dev:tenant-admin": "turbo dev --filter=@clubvantage/tenant-admin",
    "dev:marketing": "turbo dev --filter=@clubvantage/marketing",
    "dev:api": "turbo dev --filter=@clubvantage/api",
    "dev:services": "turbo dev --filter='./services/*'",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "db:migrate": "turbo db:migrate",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:seed": "turbo db:seed",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@clubvantage/config": "workspace:*",
    "prettier": "^3.2.0",
    "turbo": "^2.3.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "next-themes": "^0.4.6",
    "recharts": "^3.6.0"
  }
}
